fer(target);\n                }\n                //If apply form failed, the original id is given and we navigate to the form.\n                else if(origin != undefined){\n                    displayOffer(origin);\n                    $(\'a.apply\').trigger(\'click\');\n                }\n            }\n            $(\'.loader\').hide();\n        })\n        .error(function()\n            {\n                $(\'.loader\').hide();\n                loadJSONError(0);\n            }\n        );\n    }\n\n    navigate_to_offer = function(){\n        $.each(json_obj.offers, function(index, value){\n            if(value.id == target){\n                var go_to = index;\n                displayOffer(go_to);\n            }\n        });\n    }\n\n    loadJSONError = function(type){\n        if (type == 1){\n            $(\'.no-jobs\').show();\n        }\n        else{\n            $(\'.error\').show();\n        }\n    }\n\n    get_url = function(){\n        return \'http://www.multiposting.fr\'+base_url+\'?offer=\'+json_obj.offers[parseInt(current_offer)].id;\n    }\n\n    var base_url = null;\n    set_base_url = function(url){\n        base_url = url;\n    }\n\n    load_jobs_list = function(data){\n        $(".jobs").html("");\n        $(".jobs").show();\n        if(json_obj.offers == undefined || json_obj.offers.length == 0){\n            loadJSONError(1);\n        }\n        else {\n            $(\'.page-loader\').hide();\n            $.each(json_obj.offers,function(index,value){addOffer(index,value);});\n            $(".job_offer").click(function(){\n                return click_on_offer($(this));\n            });\n        }\n    }\n\n    set_origin = function(id){\n        origin = id;\n    }\n\n    var target = undefined;\n    set_target_offer = function(id){\n        target = id;\n    }\n\n    click_on_offer = function(offer_btn){\n        var offer_id = offer_btn.attr(\'offer_id\');\n        $(".jobs").hide();\n        displayOffer(offer_id);\n        return false;\n    }\n\n    displayOffer = function(id){\n        if(id != undefined){\n            $(".jobs").hide();\n            $("#apply_form").hide();\n            current_offer = id;\n            offer = json_obj.offers[id];\n            var job = $(".job");\n            $.each(offer,function(index,value){\n                try{\n                    $(\'#\'+index).html(value.replace(/\\n/gi,"</br>"));\n                }\n                catch(e){\n\n                 }\n            });\n            try{\n                update_share();\n            }\n            catch(e) {}\n            $(\'a.apply\').click(function(){return click_on_apply(id);});\n            job.show();\n        }\n    }\n\n    click_on_apply = function(id){\n        if(id != undefined){\n            $(".job").hide();\n            $(\'.jobs\').hide();\n            $(".apply #form_title").html(json_obj.offers[id].title);\n            $(".apply #id_mail_to").attr("value",json_obj.offers[id].email);\n            $(".apply #id_offer_id").attr("value",id);\n            $("#apply_form").show();\n            $("a.cancel").click(function(){return click_on_cancel(id);});\n            return false;\n        }\n    }\n\n    click_on_cancel = function(id){\n        $("#apply_form").hide();\n        displayOffer(id);\n        return false;\n    }\n\n    addOffer = function(index,offer){\n        build = \'<li class="job_offer" offer_id="\'+index+\'">\';\n        build +=\'<h3>\'+offer.title+\'</h3>\';\n        build += \'<a class="link" href="#"><span>\'+$(\'#offer_text\').html()+\'</span></a>\';\n        build += \'<p>\'+offer.description.replace(/\\n/gi,"</br>")+\'</p>\';\n        build += \'</li>\';\n        $(".jobs").append(build);\n    }\n\n    return_to_job_list = function(){\n        $(".job").hide();\n        $(".jobs").show();\n        $("#apply_form").hide();\n        return false;\n    }\n\n    init_file_inputs = function(){\n        if ($.browser.msie){\n            $(\'#upload-cover-letter\').html(\'<input type="file" class="msie-input" contentEditable="false" name="motivation"/>\');\n            $(\'#upload-resume\').html(\'<input type="file" class="msie-input" contentEditable="false" name="cv"/>\');\n        }\n        else{\n            $(\'input[type=file]\').change(function(e){\n                    $in=$(this);\n                    $in.parents(\'li\').find(\'input[type=text]\').attr("value",$in.val());\n            });\n        }\n    }\n\n    //Simulates a click event on dest when clicking on origin\n    redirect_click = function(orig,dest){\n        $(orig).click(function(){\n            dest.trigger(\'click\');\n            return false;\n        });\n    }\n\n    update_share = function(){\n        $(\'#twitter_share\').click(function(){\n            return pop_up(\'https://twitter.com/share?url=\'+get_url()+\'&text=\'+encodeURI(get_share_text()), \'Twitter\');\n        });\n        $(\'#twitter_share_bis\').click(function(){\n            return pop_up(\'https://twitter.com/share?url=\'+get_url()+\'&text=\'+encodeURI(get_share_text()), \'Twitter\');\n        });\n        $(\'#facebook_share\').click(function(){\n            return pop_up(\'http://www.facebook.com/sharer.php?u=\'+encodeURI(get_url()), \'Facebook\'); \n        });\n        $(\'#fb_share_bis\').click(function(){\n            return pop_up(\'http://www.facebook.com/sharer.php?u=\'+encodeURI(get_url()), \'Facebook\'); \n        });\n        $(\'#mail_share\').attr(\'href\',\'mailto:votre_contact@mail.com?body=\'+encodeURI(get_share_text()+\' \'+get_url()));\n        $(\'#viadeo_share\').click(function(){\n            return pop_up(\n                \'http://www.viadeo.com/shareit/share/?url=\'+encodeURI(get_url())+encodeURI(\'&urllanguage=fr&urlaffiliate=32001&encoding=UTF-8&urlpicture=http://www.viadeo.com/v_img12/shareit/viadeoflower.gif&title=\'+get_share_text())\n                , \'Viadeo\'\n                );\n        });\n        $(\'#google_share\').click(function(){\n            return pop_up(\'https://plus.google.com/share?url=\' + encodeURIComponent(get_url()), \'Google Plus\');\n        });\n        $(\'#linkedin_share\').click(function(){\n            return pop_up(\'https://www.linkedin.com/shareArticle?url=\' + encodeURIComponent(get_url()) + \'&title=\' + get_share_text(), \'linkedin\');\n        });\n    }\n\n    pop_up = function(url, name){\n            window.open(url, name, \'menubar=no, status=no, scrollbars=yes\'); \n            return false;\n    }\n\n    get_share_text = function(){\n        if ($(\'div.language\').attr(\'current_lang\') == \'fr\')\n        {\n            text = \'Multiposting recrute : \' + json_obj.offers[current_offer].title + \' - \';\n        }\n        else\n        {\n            text = \'Multiposting careers: \' + json_obj.offers[current_offer].title + \' - \';\n        }\n        return text;\n    }\n\n    initShare = function(){\n\n        $(\'.careers .share\').click(function() {\n            $(\'.careers .social-popin\').slideToggle(\'fast\');\n            return false;\n        });\n\n        $(\'html\').click(\n                function(){\n                    $(\'.careers .social-popin\').hide();\n                    return true;\n                }\n        );\n\n        $(\'.social\').click(function(event)\n                {\n                    event.stopPropagation();\n                }\n        );\n    }\n\n    initNavigation = function(){\n        $(\'.next-page\').click(function(){\n            if(current_offer==-1){\n                if(json_obj.offers[0] != undefined){\n                    displayOffer(0);\n                }\n            }\n            else if(json_obj.offers[parseInt(current_offer)+1] != undefined){\n                displayOffer(parseInt(current_offer)+1);\n            }\n            else{\n                return_to_job_list();\n                current_offer = -1;\n            }\n        });\n        $(\'.prev-page\').click(function(){\n            if(current_offer==-1){\n                var last = json_obj.offers.length-1;\n                if(json_obj.offers[last] != undefined){\n                    displayOffer(last);\n                }\n            }\n            else if(json_obj.offers[current_offer-1] != undefined){\n                displayOffer(current_offer-1);\n            }\n            else if (current_offer == 0){\n                return_to_job_list();\n                current_offer = -1;\n            }\n        });\n    }\n\n    var feed_url = null;\n\n    return {\n        init: function(options){\n            feed_url = options[\'feed\'];\n            $(".job").hide();\n            $("#apply_form").hide();\n            $("a.back").click(function(){return return_to_job_list();});\n            mp_loadJson();\n            initShare();\n            init_file_inputs();\n            initNavigation();\n        }\n    };\n}(jQuery));\n\n    MPCorpo.form = (function($){\n\n\n    initCountryForm = function(){\n        $(\'.country a.current\').click(function(){\n            $(\'.country ul\').toggle();\n            return false;\n        });\n        $(\'.country li a\').click(function(){\n            var c = $(this).attr(\'cc\');\n            setInputValue(c);\n            $(\'.country .current .label\').html($(this).find(\'span\').html());\n            $(\'.country ul\').hide();\n            $(this).parents(\'.country\').removeClass(\'error-field\');\n            return false;\n        });\n        $(\'.country\').click(function(event)\n            {\n                event.stopPropagation();\n            }\n        );\n\n\n    }\n\n    initAppSelector = function(){\n        $(\'ul.app-selector li\').click(function(){\n            var redirection = $(this).attr(\'section_url\');\n            location.href = redirection;\n        });\n    }\n\n    form_init = function(){\n        bind_change = function(item){\n            item.change(function(){\n                $(this).parents(\'li\').removeClass(\'error-field\');\n            });\n        }\n        bind_change($(\'input\'));\n        bind_change($(\'textarea\'));\n    }\n\n    setInputValue = function(value){\n        $(\'#id_country\').attr("value",value);\n    }\n\n    return {\n        init: function(arg){\n            if(arg !=undefined)\n            {\n                form_init();\n                if(arg[\'country\'] == true){\n                    initCountryForm();\n                }\n                if(arg[\'app-selector\'] == true){\n                    initAppSelector();\n                }\n\n            }\n        }\n    }\n\n}(jQuery));\n\n\n    set_origin();\n    set_target_offer();\n    set_base_url(\'/fr/a_propos/recrutement\');\n    MPCorpo.recruitment.init({\'feed\': \'/fr/get-job-list\'});\n    MPCorpo.form.init({\'country\':false});\n</script>\n\n</div>\n<div id="footer">\n  <div class="content">\n    <ul>\n      <li class="menu-applications"><a class="title" href="/fr/produits">Applications</a></li>\n      <li class="menu-multiposting"><a href="/fr/produits/multiposting">Multiposting</a></li>\n      <li class="menu-multisearch"><a href="/fr/produits/multisearch">Multisearch</a></li>\n      <li class="menu-adrecruiter"><a href="/fr/produits/adrecruiter">AdRecruiter</a></li>\n      <li class="menu-career-page"><a href="/fr/produits/career_page">Career Page</a></li>\n    </ul>\n    <ul>\n      <li class="menu-partners"><a class="title" href="/fr/partenaires">Partenaires</a></li>\n      <li class="menu-job-boards"><a href="/fr/partenaires/sites_emploi">Sites d\'emploi, \xc3\xa9coles</a></li>\n      <li class="menu-disability"><a href="/fr/partenaires/handicap_diversite">Handicap et diversit\xc3\xa9</a></li>\n      <li class="menu-social-networks"><a href="/fr/partenaires/reseaux_sociaux">R\xc3\xa9seaux sociaux</a></li>\n      <li class="menu-hris-developers"><a href="/fr/partenaires/editeurs_sirh">\xc3\x89diteurs de SIRH</a></li>\n    </ul>\n    <ul>\n      <li class="menu-clients"><a class="title" href="/fr/clients/nos_cli